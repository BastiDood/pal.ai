{
    "compilerOptions": {
        "noUncheckedIndexedAccess": true,
        "allowImportingTsExtensions": true,
        "noEmit": true,
        "target": "ES2022",
        "lib": [ "DOM", "ES2022" ],
        "module": "ES2020",
        "isolatedModules": true,
        "moduleResolution": "bundler",
        "strict": true,
        "types": [ "serviceworker", "w3c-image-capture" ],
        "plugins": [ { "name": "typescript-svelte-plugin" } ]
    }
}
